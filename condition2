#include<stdio.h>

int main(){
    int age;
    printf("enter your age :");
    scanf("%d ",&age);

    if(age>18){
        printf("your are an adult \n");
    }
    else if(age>13&&age<18){
        printf("your are not an teenager \n");
    }
    else{
        printf("you are a child \n");
    }
    return 0;
