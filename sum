#include<stdio.h>

int main(){
    int a,b;
    printf("enter the value of a :");
    scanf("%d",&a);

    printf("enter the value of b :");
    scanf("%d",&b);

    int sum = a + b;
    printf("sum is :%d",sum);
    //no need to declare sum for this type
    printf("sum is :%d",a + b);
    return 0;
}